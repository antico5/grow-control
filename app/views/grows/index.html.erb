<p id="notice"><%= notice %></p>

<h1>Listing Grows</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Start date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @grows.each do |grow| %>
      <tr>
        <td><%= grow.name %></td>
        <td><%= grow.start_date %></td>
        <td><%= link_to 'Show', grow %></td>
        <td><%= link_to 'Edit', edit_grow_path(grow) %></td>
        <td><%= link_to 'Destroy', grow, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Grow', new_grow_path %>
